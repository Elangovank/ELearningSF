<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/supportLayout"
    android:visibility="gone"
    tools:visibility="visible"
    style="@style/Base_Match_Wrap"
    android:layout_margin="@dimen/margin_small"
    android:orientation="vertical">
    <ScrollView

        style="@style/Base_Match_Wrap"
        >

    <LinearLayout
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_small"
        android:orientation="vertical"

        >
    <androidx.cardview.widget.CardView
        android:id="@+id/chickTypeCardView"
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_ultra_small"
        app:cardCornerRadius="@dimen/padding_small"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                style="@style/Base_Match_Wrap"
                android:layout_height="@dimen/card_height"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/padding_upper_medium">

                <ImageView
                    android:id="@+id/chickImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_chick_bird" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/padding_upper_medium"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/chickTextView"
                        android:layout_width="wrap_content"
                        android:textAlignment="viewStart"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_regular"
                        android:textColor="#128AFF"
                        android:textSize="@dimen/font_size_small"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/chickCat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_regular"
                        android:layout_marginTop="@dimen/margin_medium"
                        android:text="sub Category"
                        android:textColor="#666666"
                        android:textSize="@dimen/font_size_ultra" />


                </LinearLayout>


            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/margin_medium"
                android:src="@drawable/chickbird_bg" />


        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/feedCardView"
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_ultra_small"
        app:cardCornerRadius="@dimen/padding_small"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                style="@style/Base_Match_Wrap"
                android:layout_height="@dimen/card_height"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/padding_upper_medium">

                <ImageView
                    android:id="@+id/feedImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_regular"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/ic_feed" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/padding_upper_medium"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/feedTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/worksans_regular"
                        android:textColor="#FE4694"
                        android:textSize="@dimen/font_size_small"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/feedCat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_regular"
                        android:layout_marginTop="@dimen/margin_medium"
                        android:text="sub Category"
                        android:textColor="#666666"
                        android:textSize="@dimen/font_size_ultra" />


                </LinearLayout>


            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/margin_medium"
                android:src="@drawable/feed1_bg" />
        </RelativeLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/medicineCardView"
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_ultra_small"
        app:cardCornerRadius="@dimen/padding_small"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                style="@style/Base_Match_Wrap"
                android:layout_height="@dimen/card_height"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/padding_upper_medium">

                <ImageView
                    android:id="@+id/medicineImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_medicine" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/padding_upper_medium"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/medicineTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_regular"
                        android:textAlignment="viewStart"
                        android:textColor="#1CDA9D"
                        android:textSize="@dimen/font_size_small"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/medicineCat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_medium"
                        android:fontFamily="@font/opensans_regular"
                        android:text="sub Category"
                        android:textColor="#666666"
                        android:textSize="@dimen/font_size_ultra" />


                </LinearLayout>

            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/margin_medium"
                android:src="@drawable/medicine_bg" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/markettingCardView"
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_ultra_small"
        app:cardCornerRadius="@dimen/padding_small"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                style="@style/Base_Match_Wrap"
                android:layout_height="@dimen/card_height"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/padding_upper_medium">

                <ImageView
                    android:id="@+id/markettingImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_marketing" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/padding_upper_medium"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/markettingTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/opensans_regular"
                        android:textAlignment="viewStart"
                        android:textColor="#F98C34"
                        android:textSize="@dimen/font_size_small"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/markettingCat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_medium"
                        android:fontFamily="@font/opensans_regular"
                        android:text="sub Category"
                        android:textColor="#666666"
                        android:textSize="@dimen/font_size_ultra" />


                </LinearLayout>

            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/margin_medium"
                android:src="@drawable/marketting_bg" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/managementCardView"
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_ultra_small"
        app:cardCornerRadius="@dimen/padding_small"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                style="@style/Base_Match_Wrap"
                android:layout_height="@dimen/card_height"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/padding_upper_medium">

                <ImageView
                    android:id="@+id/managementImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_management" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/padding_upper_medium"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/managementTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/opensans_regular"
                    android:textAlignment="viewStart"
                    android:textColor="#83C831"
                    android:textSize="@dimen/font_size_small"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/managementCat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_medium"
                    android:fontFamily="@font/opensans_regular"
                    android:text="sub Category"
                    android:textColor="#666666"
                    android:textSize="@dimen/font_size_ultra" />


            </LinearLayout>

            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/margin_medium"
                android:src="@drawable/management_bg" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/othersCardView"
        style="@style/Base_Match_Wrap"
        android:layout_margin="@dimen/margin_ultra_small"
        app:cardCornerRadius="@dimen/padding_small"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                style="@style/Base_Match_Wrap"
                android:layout_height="@dimen/card_height"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/padding_upper_medium">

                <ImageView
                    android:id="@+id/othersImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_others" />

                <TextView
                    android:id="@+id/othersTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/padding_upper_medium"
                    android:fontFamily="@font/opensans_regular"
                    android:textColor="#E72F4D"
                    android:textSize="@dimen/font_size_small"
                    android:textStyle="bold" />

            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/margin_medium" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    </LinearLayout>
    </ScrollView>
</LinearLayout>
